<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="24dp">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:paddingHorizontal="24dp"
        android:background="@drawable/rect_form"
        android:gravity="center"
        tools:ignore="MissingConstraints">
        <TextView
            android:id="@+id/nomor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1"
            android:layout_marginHorizontal="12dp"
            android:fontFamily="@font/poppins_bold"
            android:textSize="16dp"/>
        <TextView
            android:id="@+id/nama"
            android:layout_width="165dp"
            android:layout_height="wrap_content"
            android:text="Nino Auliya Nahara"
            android:layout_marginEnd="8dp"
            android:fontFamily="@font/poppins_bold"
            android:textSize="16dp"/>
        <ImageView
            android:id="@+id/ikon_delete"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginHorizontal="6dp"
            android:src="@drawable/ikon_delete"/>
        <ImageView
            android:id="@+id/ikon_edit"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginHorizontal="6dp"
            android:src="@drawable/ikon_edit"/>
        <ImageView
            android:id="@+id/ikon_view"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginHorizontal="6dp"
            android:src="@drawable/ikon_view"/>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>